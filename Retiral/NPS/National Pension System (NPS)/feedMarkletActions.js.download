




















 var feedMarkletActions = {

	actions:[
		
		{
		'href':"javascript:feedMarkletActions.feedPopUp(\'%l\')",
		'text':"Add to Communities"
		}, 
		{'href':"%l",
		'text':"Subscribe in Your Browser"}],
	feedPopUp:function(location) {
		var urlStr = encodeURIComponent(location);
		
		window.open('https://connect.bosch.com/connections/bookmarklet/postFeed?lang=en_US&addToCommunities=true&ver=ignore&url='+urlStr, 'feed', 'toolbars=no,scrollbars=yes,resizable=yes,width=670,height=600');
		
	}
}
